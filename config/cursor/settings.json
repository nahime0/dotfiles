{
    "window.commandCenter": true,
    "vim.neovimUseConfigFile": false,
    "vim.normalModeKeyBindings": [],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "k",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        }
    ],
    "editor.fontLigatures": "'calt', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09'",
    "window.newWindowDimensions": "inherit",
    "editor.fontSize": 16,
    "editor.lineHeight": 1.8,
    "editor.smoothScrolling": true,
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.preferredLightColorTheme": "Catppuccin Latte",
    "terminal.integrated.fontSize": 16,
    "jetbrainsKeymap.promptV3Features": false,
    "files.autoSave": "onFocusChange",
    "LaravelExtraIntellisense.modelVariables": {
        "user": "App\\Models\\User"
    },
    "LaravelExtraIntellisense.modelsPaths": [
        "app/Models"
    ],
    "intelephense.environment.documentRoot": "public/index.php",
    "intelephense.environment.includePaths": [
        "./vendor/_laravel_idea"
    ],
    "php.validate.enable": true,
    "php.suggest.basic": true,
    "phpTools.suppressPremiumFeatures": true,
    "git.confirmSync": false,
    "workbench.tree.indent": 20,
    "workbench.editor.enablePreview": false,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 20,
    "workbench.sideBar.location": "left",
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "editor.lineNumbers": "relative",
    "laravel-pint.enable": true,
    "editor.formatOnSave": true,
    "workbench.list.openMode": "doubleClick",
    "workbench.activityBar.orientation": "vertical",
    "terminal.integrated.fontFamily": "'ComicShannsMono Nerd Font Mono', 'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
    "files.dialog.defaultPath": "/Users/nahime/Developer",
    "window.autoDetectColorScheme": true,
    "workbench.preferredDarkColorTheme": "Catppuccin Macchiato",
    "explorer.autoReveal": false,
    "workbench.iconTheme": "catppuccin-macchiato",
    "editor.guides.indentation": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "'ComicShannsMono Nerd Font Mono', 'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
    "database-client.autoSync": true,
    "cursor.composer.shouldAllowCustomModes": true,
    "kotlin.languageServer.enabled": false,
    "kotlin.debugAdapter.enabled": false,
    "Laravel.phpEnvironment": "herd",
    "terminal.integrated.lineHeight": 1.2,
    "gitlens.ai.model": "gitkraken",
    "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
    "[php]": {
        "editor.defaultFormatter": "open-southeners.laravel-pint"
    },
    "claudeCode.selectedModel": "default",
    "claudeCode.preferredLocation": "sidebar"
}